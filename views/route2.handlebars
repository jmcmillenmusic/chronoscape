<div class="slide" data-user_id="{{user.id}}">
</div>

{{!-- Shows first quesiton --}}
<div id="parentAnswer-container">
  <h2 class="main-question">Question:</h2>
  <h2 class="main-question"> {{questions.[4].question}}</h2>
  <ul class="answerChoices">
    {{#each questions.[4].answers}}
      <li class="parentAnswer" style="display: inline-block;">
        <Button class="card">
          <div class="content-container">
            <p>{{this.answerChoice}}</p>
            <div class="darken-cards next card__face card__face--front">
              <img class="cardImg" src="{{this.imgLink}}"  data-answer_id={{this.id}}></img>
            </div>
          </div>
        </Button>
      </li>
    {{/each}}
  </ul>
</div>

<div id="parentLocation-container" style="display: none;">
</div>

<div id="childAnswer-container" style="display: none;">
</div>

<div id="childLocation-container" style="display: none;">
</div>

<script src="/js/homepage.js"></script>
<script type="module" src="/js/homepage2.js"></script>

